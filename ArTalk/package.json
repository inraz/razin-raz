{
  "name": "artalk",
  "version": "1.0.0",
  "description": "This is awesome vedio call feture",
  "main": "server.js",
  "scripts": {
    "start": "node app/src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx prettier --write .",
    "docker-build": "docker build --tag artalk/p2p:latest .",
    "docker-rmi": "docker images |grep '<none>' |awk '{print $3}' |xargs docker rmi",
    "docker-push": "docker push artalk/p2p:latest",
    "docker-pull": "docker pull artalk/p2p:latest",
    "docker-rmc": "docker container rm artalk --force",
    "docker-run": "docker run -d -p 3000:3000 --name artalk artalk/p2p:latest",
    "docker-rune": "docker run -d -p 3000:3000 -v .env:/src/.env:ro --name artalk artalk/p2p:latest",
    "docker-run-vm": "docker run -d -p 3000:3000 -v .env:/src/.env:ro -v ./app/:/src/app/:ro -v ./public/:/src/public/:ro --name artalk artalk/p2p:latest",
    "docker-start": "docker start artalk",
    "docker-stop": "docker stop artalk"
  },
  "keywords": [
    "webrtc",
    "socket.io",
    "p2p",
    "nodejs",
    "video"
  ],
  "author": "ArTalk",
  "license": "AGPL-3.0",
  "dependencies": {
    "@sentry/integrations": "^7.91.0",
    "@sentry/node": "^7.91.0",
    "axios": "^1.6.3",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ngrok": "^5.0.0-beta.2",
    "openai": "^4.24.1",
    "qs": "^6.11.2",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "xss": "^1.0.14",
    "yamljs": "^0.3.0"
  }
}
